.wrap{display:flex;height:100vh;overflow:hidden}.wrap_province{flex-shrink:0;width:100px;padding-bottom:constant(safe-area-inset-bottom);padding-bottom:env(safe-area-inset-bottom)}.wrap_province .wrap_province_item{height:48px;display:flex;align-items:center;justify-content:center;color:rgba(255,255,255,0.6);position:relative}.wrap_province .wrap_province_item.highlight{color:#4D6DF7;font-weight:bold;background:rgba(255,255,255,0.15)}.wrap_province .wrap_province_item .mark{position:absolute;right:4px;bottom:0;top:calc((48px - 16px) / 2);width:16px;height:16px;border-radius:50%;background:#461CE5;color:#fff;font-size:10px;display:flex;justify-content:center;align-items:center}.wrap_city{flex:1;height:100vh;overflow:hidden;display:flex;flex-direction:column;background:rgba(255,255,255,0.15)}.wrap_city .wrap_city_list{flex:1}.wrap_city .wrap_city_list .wrap_city_list-inner{display:flex;flex-wrap:wrap;padding:16px 16px 16px 4px}.wrap_city .wrap_city_list .wrap_city_list-inner .wrap_city_list-inner-item{width:calc((100% - 24px - 6px - 12px) / 3);height:32px;font-size:13px;margin:0 0 12px 12px;display:flex;justify-content:center;align-items:center;border:1px dashed rgba(255,255,255,0.6);border-radius:4px;position:relative}.wrap_city .wrap_city_list .wrap_city_list-inner .wrap_city_list-inner-item.highlight{border:1px solid #461CE5}.wrap_city .wrap_city_list .wrap_city_list-inner .wrap_city_list-inner-item .choose{position:absolute;left:-8px;top:-8px;width:16px;height:16px}.wrap_city .wrap_city_list .wrap_city_list-inner .wrap_city_list-inner-item .choose_icon{width:100%;height:100%}.wrap_city .wrap_city_submit{flex-shrink:0;display:flex;justify-content:center;padding-bottom:constant(safe-area-inset-bottom);padding-bottom:env(safe-area-inset-bottom)}.wrap_city .wrap_city_submit-btn{margin-top:16px;margin-bottom:16px;width:128px;display:flex;justify-content:center;width:108px;background:#461CE5;padding:8px 32px;border-radius:4px;background:transparent;border:1px solid #461CE5}